type Ret(x: void -> A, y: void -> A) #unboxed;
class A { }
class B extends A { }
def main(x: int) -> bool {
	var t = m1()();
	return t.x() == t.y();
}
def m1() -> void -> Ret {
	return ab;
}
def ab() -> Ret { return Ret(ma, mb); }
