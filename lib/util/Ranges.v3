// Utility methods for Ranges.
component Ranges {
	// Make a copy of a range (as an array).
	def dup<T>(r: Range<T>) -> Array<T> {
		var result = Array<T>.new(r.length);
		for (i < r.length) result[i] = r[i];
		return result;
	}
}
