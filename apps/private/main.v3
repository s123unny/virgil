// A simple test of the visibility of private types.

def main() -> int {
	return A.new().fooA + C.fooC + T.FooT.m() + E.FOO.tag + X.new().foo() + X.new().bar;
}
private class A {
	def fooA = 99;
}
private component C {
	def fooC = 101;
}
private type T {
	case FooT;

	def m() -> int { return 0; }
}
private enum E {
	FOO
}

class X {
	private def bar: int = 99;
	private def foo() -> int { return 100; }
}
